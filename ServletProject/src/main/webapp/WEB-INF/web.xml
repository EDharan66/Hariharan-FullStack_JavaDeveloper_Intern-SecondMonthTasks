<?xml version="1.0" encoding="UTF-8"?>
<web-app 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
version="4.0">
  <display-name>ServletProject</display-name>
  
 <servlet>
<servlet-name>firstServlet</servlet-name>
<servlet-class>Demo.FirstServlet</servlet-class>
</servlet>
<servlet>
<servlet-name>ServletInterface</servlet-name>
<servlet-class>ServletInterfacePackage.ServletInterfaceExample</servlet-class>
</servlet>
<servlet>
<servlet-name>HttpServlet</servlet-name>
<servlet-class>HttpServletPackage.HttpServletExample</servlet-class>
</servlet>
<servlet>
<servlet-name>GenericServlet</servlet-name>
<servlet-class>GenericServletPackage.GenericServletExample</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>firstServlet</servlet-name>
<url-pattern>/start</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>ServletInterface</servlet-name>
<url-pattern>/ServletInterfaceExample</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>GenericServlet</servlet-name>
<url-pattern>/GenericServletClassExample</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>HttpServlet</servlet-name>
<url-pattern>/HttpServletClassExample</url-pattern>
</servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  

</web-app>